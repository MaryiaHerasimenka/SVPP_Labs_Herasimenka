<Window x:Class="BD_Lab6_Herasimenka.EditTrackWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Edit Track" Height="300" Width="400">
    <Grid>
        <StackPanel>
            <Label Content="Track Title:"/>
            <TextBox x:Name="txtTrackTitle"/>
            <Label Content="Track Duration:"/>
            <TextBox x:Name="txtTrackDuration"/>
            <Label Content="Artist Name:"/>
            <TextBox x:Name="txtArtistName"/>
            <Label Content="Album Title:"/>
            <TextBox x:Name="txtAlbumTitle"/>
            <Button Margin="40" Content="Save" Click="Save_Click"/>
        </StackPanel>
    </Grid>
</Window>
